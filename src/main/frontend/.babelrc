{
	"assumptions": {
		"setPublicClassFields": true
	},
	"presets": [
		[
			"@babel/env",
			{
				"debug": true,
				"targets": {
					"esmodules": true
				},
				"modules": false,
				"useBuiltIns": false
			}
		],
		"@babel/typescript"
	],
	"plugins": [

		[
			"@babel/proposal-decorators",
			{
				"decoratorsBeforeExport": true
			}
		],
		[
			"@babel/proposal-class-properties",
			{
			}
		],
		"@babel/proposal-object-rest-spread",
		[
			"@babel/transform-runtime",
			{
				"corejs": false,
				"helpers": true,
				"regenerator": true,
				"useESModules": true
			}
		]
	]
}