{
	"name": "web-player-js",
	"version": "1.0.0",
	"description": "Web player for lecture/conference/classroom recordings",
	"private": true,
	"scripts": {
		"build:dev": "webpack --config webpack.dev.js",
		"build:prod": "SET NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.prod.js",
		"watch": "concurrently \"webpack --config webpack.dev.js --watch\" \"npm run type-check:watch\"",
		"start:dev": "webpack serve --host 0.0.0.0 --config webpack.dev.js",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"generate-docs": "compodoc -p tsconfig.json"
	},
	"keywords": [
		"recording",
		"stream",
		"pdf",
		"player",
		"viewer"
	],
	"author": "Alex Andres",
	"license": "Apache-2.0",
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.13.8",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-decorators": "^7.13.5",
		"@babel/plugin-proposal-object-rest-spread": "^7.13.8",
		"@babel/plugin-transform-runtime": "^7.13.8",
		"@babel/preset-env": "^7.13.8",
		"@babel/preset-typescript": "^7.13.0",
		"angular2-template-loader": "^0.6.2",
		"autoprefixer": "^10.2.4",
		"babel-loader": "^8.2.2",
		"bootstrap": "^5.0.0-beta2",
		"clean-webpack-plugin": "^1.0.1",
		"concurrently": "^6.0.0",
		"css-loader": "^5.1.0",
		"exports-loader": "^3.0.0",
		"postcss": "^8.2.6",
		"postcss-loader": "^5.0.0",
		"resize-observer-polyfill": "^1.5.1",
		"style-loader": "^2.0.0",
		"tailwindcss": "^2.0.3",
		"to-string-loader": "^1.1.5",
		"typescript": "^4.3.5",
		"webpack": "^5.24.2",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.7.3",
		"worker-loader": "^3.0.8"
	},
	"dependencies": {
		"@types/node": "^10.12.18",
		"i18next": "^21.6.12",
		"i18next-browser-languagedetector": "^6.1.3",
		"janus-gateway": "git://github.com/meetecho/janus-gateway.git",
		"lit": "^2.2.0",
		"lottie-web": "^5.7.12",
		"pdfjs-dist": "^2.6.347"
	}
}
