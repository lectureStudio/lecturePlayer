{
	"name": "web-player-js",
	"version": "2.0.0",
	"description": "Web player for lecture/conference/classroom recordings",
	"private": true,
	"scripts": {
		"build:dev": "webpack --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js",
		"watch": "concurrently \"webpack --config webpack.dev.js --watch\" \"npm run type-check:watch\"",
		"start:dev": "webpack serve --host 0.0.0.0 --config webpack.dev.js",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"generate-docs": "compodoc -p tsconfig.json"
	},
	"keywords": [
		"recording",
		"stream",
		"pdf",
		"player",
		"viewer"
	],
	"author": "Alex Andres",
	"license": "Apache-2.0",
	"devDependencies": {
		"@alienfast/i18next-loader": "^1.1.4",
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.5",
		"@babel/plugin-proposal-class-properties": "^7.16.7",
		"@babel/plugin-proposal-decorators": "^7.17.2",
		"@babel/plugin-proposal-object-rest-spread": "^7.17.3",
		"@babel/plugin-transform-runtime": "^7.17.0",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@types/node": "^10.12.18",
		"@types/toastify-js": "^1.11.0",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^1.0.1",
		"concurrently": "^6.0.0",
		"copy-webpack-plugin": "^11.0.0",
		"css-loader": "^6.7.3",
		"exports-loader": "^3.0.0",
		"lit-css-loader": "^2.0.0",
		"mini-css-extract-plugin": "^2.7.2",
		"resize-observer-polyfill": "^1.5.1",
		"sass": "^1.53.0",
		"sass-loader": "^13.0.1",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.3.1",
		"typescript": "^4.6.2",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.5.0",
		"webpack-dev-server": "^4.6.0",
		"webpack-merge": "^5.7.3",
		"worker-loader": "^3.0.8"
	},
	"dependencies": {
		"@shoelace-style/shoelace": "^2.2.0",
		"@stomp/stompjs": "^6.1.2",
		"i18next": "^21.8.12",
		"i18next-browser-languagedetector": "^6.1.4",
		"janus-gateway": "git://github.com/meetecho/janus-gateway.git",
		"lit": "^2.2.7",
		"pdfjs-dist": "^2.16.105"
	}
}
